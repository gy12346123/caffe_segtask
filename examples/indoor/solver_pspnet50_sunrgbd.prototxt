# solver
net: "./train_pspnet50_sunrgbd.prototxt"

test_iter: 100
test_interval: 10000
test_initialization: false

lr_policy: "poly"
gamma: 0.1
base_lr: 0.01 # 1e-4 # 0.00001
power: 0.9
stepsize: 20000

average_loss: 20
display: 100
max_iter: 200000
momentum: 0.9
weight_decay: 0.0005

snapshot: 20000
snapshot_prefix: "snapshots/pspnet50-SUNRGBD"
solver_mode: GPU
device_id: [0,1,2,3,4]
